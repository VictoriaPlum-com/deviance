{
  "name": "deviance-example",
  "version": "1.0.0",
  "description": "Example site for deviance and nightwatch",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "server": "http-server",
    "bump": "yarn upgrade deviance",
    "sym": "cd ../ && yarn link && cd example && yarn link deviance",
    "nightwatch": "yarn sym && nightwatch",
    "example": "concurrently -k -s first -r \"npm:server\" \"npm:nightwatch\"",
    "example:chrome": "concurrently -k -s first -r \"npm:server\" \"npm:nightwatch -- -e chrome\""
  },
  "dependencies": {
    "chromedriver": "^2.40.0",
    "concurrently": "^3.6.0",
    "deviance": "./..",
    "geckodriver": "^1.11.0",
    "http-server": "^0.11.1",
    "nightwatch": "1.0.6",
    "npm-delay": "^1.0.4",
    "selenium-server-standalone-jar": "^3.13.0"
  }
}
